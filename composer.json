{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.2",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "lexik/jwt-authentication-bundle": "2.5",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/asset": "4.0.*",
        "symfony/console": "4.0.*",
        "symfony/dotenv": "4.0.*",
        "symfony/expression-language": "4.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.0.*",
        "symfony/framework-bundle": "4.0.*",
        "symfony/maker-bundle": "1.33.*",
        "symfony/property-access": "4.0.*",
        "symfony/property-info": "4.0.*",
        "symfony/proxy-manager-bridge": "4.0.*",
        "symfony/security-bundle": "4.0.*",
        "symfony/serializer": "4.0.*",
        "symfony/twig-bundle": "4.0.*",
        "symfony/validator": "4.0.*",
        "symfony/yaml": "4.0.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "reset-db": [
        "php bin/console doctrine:schema:drop --force; php bin/console doctrine:schema:create; php bin/console doctrine:fixtures:load -q;"
      ],
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.0.*"
        }
    }
}
